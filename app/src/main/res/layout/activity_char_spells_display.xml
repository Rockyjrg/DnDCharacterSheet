<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/normal"
                android:gravity="center"
                android:text="Paladin's Spells and Cantrips Sheet" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="12dp" />

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="699dp"
                android:background="@color/normal">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:layout_marginRight="5dp"
                            android:background="@color/normal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center|left"
                                android:background="@color/normal"
                                android:backgroundTint="@color/normal"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/lvl9"
                                    android:layout_width="20dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_weight="1"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/disable"
                                    android:gravity="center"
                                    android:text="0" />

                                <TextView
                                    android:id="@+id/spellName9"
                                    android:layout_width="90dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="Cantrips" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/spellDescriptor9"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:background="@color/disable"
                                android:gravity="top|left"
                                android:text="Fire Bolt" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:background="@color/normal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center|left"
                                android:background="@color/normal"
                                android:backgroundTint="@color/normal"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/lvl10"
                                    android:layout_width="37dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_weight="1"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/disable"
                                    android:gravity="center"
                                    android:text="1" />

                                <TextView
                                    android:id="@+id/spellName10"
                                    android:layout_width="116dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/spellDescriptor10"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:background="@color/disable"
                                android:gravity="top|left"
                                android:text="Gust" />

                        </LinearLayout>

                    </LinearLayout>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:layout_marginRight="5dp"
                            android:background="@color/normal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center|left"
                                android:background="@color/normal"
                                android:backgroundTint="@color/normal"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/lvl11"
                                    android:layout_width="20dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_weight="1"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/disable"
                                    android:gravity="center"
                                    android:text="2" />

                                <TextView
                                    android:id="@+id/spellName11"
                                    android:layout_width="90dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/spellDescriptor11"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:background="@color/disable"
                                android:gravity="top|left"
                                android:text="Meld into Stone" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:background="@color/normal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center|left"
                                android:background="@color/normal"
                                android:backgroundTint="@color/normal"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/lvl12"
                                    android:layout_width="37dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_weight="1"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/disable"
                                    android:gravity="center"
                                    android:text="3" />

                                <TextView
                                    android:id="@+id/spellName12"
                                    android:layout_width="116dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/spellDescriptor12"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:background="@color/disable"
                                android:gravity="top|left"
                                android:text="Wind wall" />

                        </LinearLayout>

                    </LinearLayout>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="5dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="left"
                                android:layout_marginRight="5dp"
                                android:background="@color/normal"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/normal"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/lvl13"
                                        android:layout_width="20dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center|left"
                                        android:layout_weight="1"
                                        android:background="@color/normal"
                                        android:backgroundTint="@color/disable"
                                        android:gravity="center"
                                        android:text="4" />

                                    <TextView
                                        android:id="@+id/spellname13"
                                        android:layout_width="90dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:layout_weight="1"
                                        android:gravity="center" />
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/spellDescriptor13"
                                    android:layout_width="match_parent"
                                    android:layout_height="120dp"
                                    android:background="@color/disable"
                                    android:gravity="top|left"
                                    android:text="Blight" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:layout_gravity="left"
                                android:background="@color/normal"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/normal"
                                    android:gravity="center"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/lvl14"
                                        android:layout_width="37dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center|left"
                                        android:layout_weight="1"
                                        android:background="@color/normal"
                                        android:backgroundTint="@color/disable"
                                        android:gravity="center"
                                        android:text="5" />

                                    <TextView
                                        android:id="@+id/spellName14"
                                        android:layout_width="116dp"
                                        android:layout_height="30dp"
                                        android:layout_gravity="center"
                                        android:layout_weight="1"
                                        android:gravity="center" />
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/spellDescriptor14"
                                    android:layout_width="match_parent"
                                    android:layout_height="120dp"
                                    android:background="@color/disable"
                                    android:gravity="top|left"
                                    android:text="Wall of Fire" />

                            </LinearLayout>

                        </LinearLayout>
                    </TableRow>

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:layout_marginRight="5dp"
                            android:background="@color/normal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center|left"
                                android:background="@color/normal"
                                android:backgroundTint="@color/normal"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/lvl15"
                                    android:layout_width="20dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_weight="1"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/disable"
                                    android:gravity="center"
                                    android:text="6" />

                                <TextView
                                    android:id="@+id/spellName15"
                                    android:layout_width="90dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/spellDescriptor15"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:background="@color/disable"
                                android:gravity="top|left"
                                android:text="Heal" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="200dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="left"
                            android:background="@color/normal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="30dp"
                                android:layout_gravity="center|left"
                                android:background="@color/normal"
                                android:backgroundTint="@color/normal"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/lvl16"
                                    android:layout_width="37dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center|left"
                                    android:layout_weight="1"
                                    android:background="@color/normal"
                                    android:backgroundTint="@color/disable"
                                    android:gravity="center"
                                    android:text="7" />

                                <TextView
                                    android:id="@+id/spellName16"
                                    android:layout_width="116dp"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center"
                                    android:layout_weight="1"
                                    android:gravity="center" />
                            </LinearLayout>

                            <TextView
                                android:id="@+id/spellDescription16"
                                android:layout_width="match_parent"
                                android:layout_height="120dp"
                                android:background="@color/disable"
                                android:gravity="center|left"
                                android:text="Awaken" />

                        </LinearLayout>

                    </LinearLayout>
                </TableRow>

            </TableLayout>
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>